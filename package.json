{
  "name": "@aureooms/js-codec-base64",
  "description": "Base64 encoder/decoder for JavaScript",
  "version": "1.0.0",
  "author": "aureooms",
  "ava": {
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-codec-base64/issues"
  },
  "dependencies": {
    "@aureooms/js-codec": "^2.0.0",
    "@aureooms/js-error": "^3.0.0",
    "@aureooms/js-itertools": "^3.4.0",
    "@aureooms/js-mapping": "^3.1.0"
  },
  "devDependencies": {
    "@aureooms/js-codec-ascii": "^1.0.1",
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/register": "7.0.0",
    "ava": "^0.25.0",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.2",
    "esdoc": "^1.1.0",
    "np": "^3.0.4",
    "nyc": "^13.1.0"
  },
  "homepage": "https://aureooms.github.io/js-codec-base64",
  "keywords": [
    "base64",
    "codec",
    "decode",
    "encode",
    "strings"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/js-codec-base64"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava ./test/src"
  }
}
